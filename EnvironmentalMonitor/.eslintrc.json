{
    "env": {
        "browser": true,
        "es6": true,
        "commonjs": true,
        "amd": true,
        "node": true
    },
    "extends": [
        "eslint:recommended", 
        "plugin:vue/essential"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "parser":"babel-eslint"
    },
    "plugins": [
        "vue"
    ]
}